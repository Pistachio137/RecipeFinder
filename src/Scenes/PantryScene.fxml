<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.MainSceneController">
   <top>
      <ToolBar BorderPane.alignment="CENTER">
        <items>
          <Button mnemonicParsing="false" onAction="#homepage" text="Back" />
            <TextField />
        </items>
      </ToolBar>
   </top>
   <center>
      <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <ComboBox fx:id="pantryCB1" editable="true" onAction="#addToPantryCB" promptText="Add items..." />
                  <Button fx:id="addPantryItemCB" mnemonicParsing="false" onAction="#addToPantryCB" text="Add" />
               </children>
            </HBox>
            <HBox fx:id="pantryHBox2" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <children>
                  <ScrollPane fx:id="pantrySP1" prefHeight="800.0" prefWidth="900.0">
                     <content>
                        <FlowPane fx:id="pantryFP1" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                           <children>
                              <Pagination fx:id="pantryPG1">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </Pagination>
                           </children>
                        </FlowPane>
                     </content>
                  </ScrollPane>
                  <ScrollPane fx:id="pantrySP2" prefHeight="800.0" prefWidth="900.0">
                     <content>
                        <FlowPane fx:id="pantryFP2" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
                     </content>
                  </ScrollPane>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
